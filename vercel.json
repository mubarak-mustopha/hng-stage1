{
    "version": 2,
    "builds": [
        {
            "src": "hng-stage1/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "hng-stage1/wsgi.py"
        }
    ]
  }